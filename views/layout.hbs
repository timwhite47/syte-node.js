<script type="text/javascript">

{{#if syteSettings.integrations.woopra.enabled}}
function woopraReady(tracker) {
    tracker.setDomain("{{syteSettings.integrations.woopra.trackingDomain}}");
    tracker.setIdleTimeout({{syteSettings.integrations.woopra.idleTimeout}});
    {{#if syteSettings.integrations.woopra.includeQuery}}
    tracker.trackPageview({type:'pageview',url:window.location.pathname+window.location.search,title:document.title});
    {{else}}
    tracker.track();
    {{/if}}

    return false;
}
(function() {
    var wsc = document.createElement('script');
    wsc.src = document.location.protocol+'//static.woopra.com/js/woopra.js';
    wsc.type = 'text/javascript';
    wsc.async = true;
    var ssc = document.getElementsByTagName('script')[0];
    ssc.parentNode.insertBefore(wsc, ssc);
})();
{{/if}}

{{#if syteSettings.integrations.googleAnalytics.enabled}}
var _gaq = _gaq || [];
_gaq.push(['_setAccount', '{{syteSettings.integrations.googleAnalytics.trackingId}}']);
_gaq.push(['_trackPageview']);
(function() {
var ga = document.createElement('script'); ga.type = 'text/javascript'; ga.async = true;
ga.src = ('https:' == document.location.protocol ? 'https://ssl' : 'http://www') + '.google-analytics.com/ga.js';
var s = document.getElementsByTagName('script')[0]; s.parentNode.insertBefore(ga, s);
})();
{{/if}}
</script>
